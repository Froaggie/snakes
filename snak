squamata_instance = squamata4("squamata")
pythonidae_instance = pythonidae5("Family pythonidae")
python_instance = python6("Genus python")
morelia_instance = morelia6("Genus morelia")
malayopython_instance = malayopython6("Genus malayopython")
python_regius_instance = Python_regius7("Species ball python")
Python_molurus_instance = Python_molurus7("Species indian python")
Python_sebae_instance = Python_sebae7("Species central african rock python")
Python_bivittatus_instance = Python_bivittatus7("Species burmese python")
Python_natalensis_instance = Python_natalensis7("Species south african rock python")
Python_curtus_instance = Python_curtus7("Species sumatran short-tailed python")
Python_breitensteini_instance = Python_breitensteini7("Species borneo python")
Python_brongersmai_instance = Python_brongersmai7("Species blood python")
Morelia_kinghorni_instance = Morelia_kinghorni7("Species australian scrub python")
Morelia_spilota_instance = Morelia_spilota7("Species carpet python")
Morelia_viridis_instance = Morelia_viridis7("Species green tree python")
Malayopython_timoriensis_instance = Malayopython_timoriensis7("Species timor python")

def ball():
    python_regius_instance.animal_properties()
    print("\n" * 2)

def indian():
    Python_molurus_instance.animal_properties()
    print("\n" *2)

def centralrock():
    Python_sebae_instance.animal_properties()
    print("\n" *2)

def burmese():
    Python_bivittatus_instance.animal_properties()
    print("\n" *2)

def southrock():
    Python_natalensis_instance.animal_properties()
    print("\n" *2)

def sumatran():
    Python_curtus_instance()
    print("\n" *2)

def borneo():
    Python_breitensteini_instance.animal_properties()
    print("\n" *2)

def blood():
    Python_brongersmai_instance.animal_properties()
    print("\n" *2)

def aussie():
    Morelia_kinghorni_instance.animal_properties()
    print("\n" *2)

def carpet():
    Morelia_spilota_instance.animal_properties()
    print("\n" *2)

def greentree():
    Morelia_viridis_instance.animal_properties()
    print("\n" *2)

def timor():
    Malayopython_timoriensis_instance.animal_properties()
    print("\n" *2)

def mainmenu():
    print("======== WELCOME TO PYTHON PICKER ========")
    print("Please pick the python you would like to know about!")
    print(" 1. Ball python \n 2. Indian python \n 3. Central african rock python \n 4. Burmese python \n 5. Southern african rock python \n 6. Sumatran short-tailed python \n 7. Borneo python \n 8. Blood python \n 9. Australian scrub python \n 10. Carpet python \n 11. Green tree python \n 12. Timor python")
    picked_snake = input()
    snakes = {'1': ball, '2': indian, '3': centralrock, '4': burmese, '5': southrock, '6': sumatran, '7': borneo, '8': blood, '9':aussie, '10': carpet, '11': greentree, '12':timor}
    if picked_snake in snakes:
        snakes[picked_snake]()
        mainmenu()
    else:
        print("Sorry! that snake is not in our index, pick a different one!")
        mainmenu()


mainmenu()
